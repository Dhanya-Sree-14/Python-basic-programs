   "source": [
    "# Python program to find the factorial of a number \n",
    "\n",
    "num = int(input(\"Enter a number:\"))\n",
    "\n",
    "fact= 1\n",
    "\n",
    "\n",
    "if num<0:\n",
    "   print(\"factorial does not exist for negative numbers\")\n",
    "else:\n",
    "   for i in range(1,num + 1):\n",
    "       fact = fact*i\n",
    "   print(\"The factorial of\",num,\"is\",fact)"
   ]
